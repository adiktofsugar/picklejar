{
  "name": "picklejar",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "concurrently npm:lint:*",
    "lint:eslint": "eslint .",
    "lint:tsc": "tsc -b --noEmit",
    "lint:prettier": "prettier --check .",
    "fix": "concurrently npm:fix:*",
    "fix:eslint": "eslint . --fix",
    "fix:prettier": "prettier --check --write .",
    "preview": "vite preview",
    "types": "concurrently npm:types:*",
    "types:wrangler": "wrangler types ./wrangler-types.d.ts",
    "types:router": "tsr generate",
    "types:codegen": "graphql-codegen --config graphql.config.ts",
    "db:prepare": "wrangler d1 migrations apply picklejar"
  },
  "dependencies": {
    "@apollo/client": "^4.0.11",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@picocss/pico": "^2.1.1",
    "@tanstack/react-router": "^1.144.0",
    "aws4fetch": "^1.0.20",
    "graphql": "^16.12.0",
    "graphql-yoga": "^5.18.0",
    "hono": "^4.11.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-hook-form": "^7.69.0",
    "rxjs": "^7.8.2"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "^1.19.0",
    "@eslint/js": "^9.39.1",
    "@graphql-codegen/cli": "^6.1.0",
    "@graphql-codegen/typed-document-node": "^6.1.5",
    "@graphql-codegen/typescript": "^5.0.7",
    "@graphql-codegen/typescript-operations": "^5.0.7",
    "@graphql-codegen/typescript-resolvers": "^5.1.5",
    "@tanstack/router-cli": "^1.145.2",
    "@tanstack/router-plugin": "^1.145.2",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "concurrently": "^9.2.1",
    "eslint": "^9.39.1",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import-x": "^4.16.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "graphql-config": "^5.1.5",
    "prettier": "^3.7.4",
    "sass-embedded": "^1.97.1",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.4",
    "vite-plugin-graphql-codegen": "^3.8.0",
    "wrangler": "^4.56.0"
  }
}
